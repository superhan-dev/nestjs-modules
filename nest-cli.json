{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-modules/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestjs-modules/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestjs-modules",
  "projects": {
    "nestjs-modules": {
      "type": "application",
      "root": "apps/nestjs-modules",
      "entryFile": "main",
      "sourceRoot": "apps/nestjs-modules/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-modules/tsconfig.app.json"
      }
    },
    "typeorm": {
      "type": "application",
      "root": "apps/typeorm",
      "entryFile": "main",
      "sourceRoot": "apps/typeorm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/typeorm/tsconfig.app.json"
      }
    },
    "uploader": {
      "type": "application",
      "root": "apps/uploader",
      "entryFile": "main",
      "sourceRoot": "apps/uploader/src",
      "compilerOptions": {
        "tsConfigPath": "apps/uploader/tsconfig.app.json"
      }
    },
    "graphql-mongodb": {
      "type": "application",
      "root": "apps/graphql-mongodb",
      "entryFile": "main",
      "sourceRoot": "apps/graphql-mongodb/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graphql-mongodb/tsconfig.app.json"
      }
    },
    "browser-storages": {
      "type": "application",
      "root": "apps/browser-storages",
      "entryFile": "main",
      "sourceRoot": "apps/browser-storages/src",
      "compilerOptions": {
        "tsConfigPath": "apps/browser-storages/tsconfig.app.json"
      }
    }
  }
}